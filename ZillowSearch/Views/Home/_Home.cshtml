<!-- ko with: home -->
<div class="jumbotron horizontal-center">
    <div class="col-md-12">
        <h1>ZillowSearch</h1>
        <p class="lead">Get started with entering an address. ZillowSearch will find the information for you. </p>
            @using (Html.BeginForm("SearchResults", "Home", FormMethod.Post))
            {
              <div class="container">
                  <div class="form-group col-md-6 col-md-offset-3">
                      <input type="text" name="SearchStreet" class="form-control" placeholder="Enter a Street Name">
                  </div>
                    <div class="form-group col-md-2 col-md-offset-3">
                        <input type="text" name="SearchCity" class="form-control" placeholder="Enter a City">
                    </div>
                    <div class="form-group col-md-2">
                        <input type="text" name="SearchState" class="form-control" placeholder="Enter a State">
                    </div>
                    <div class="form-group col-md-2">
                        <input type="text" name="SearchZIP" class="form-control" placeholder="Enter ZIP">
                    </div>

              </div>
            <div class="col-md-6 col-md-offset-3">
                <span class="input-group-btn">
                    <input class="btn btn-default btn-primary" type="submit" value="Go" />
                </span>
            </div>
           }
    </div>
 </div>



<!-- /ko -->
